<h1>Listing rating_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rating_types.each do |rating_type| %>
      <tr>
        <td><%= rating_type.name %></td>
        <td><%= rating_type.rate %></td>
        <td><%= link_to 'Show', rating_type %></td>
        <td><%= link_to 'Edit', edit_rating_type_path(rating_type) %></td>
        <td><%= link_to 'Destroy', rating_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating type', new_rating_type_path %>
